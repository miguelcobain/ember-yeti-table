{
  "name": "test-app",
  "version": "2.0.0",
  "description": "Lightweight and flexible tables addon.",
  "keywords": [
    "table",
    "tables",
    "datagrid",
    "yeti"
  ],
  "homepage": "https://miguelcobain.github.io/ember-yeti-table",
  "repository": "https://github.com/miguelcobain/ember-yeti-table",
  "license": "MIT",
  "author": "Miguel Andrade <jmandrade0@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "deploy": "ember deploy production",
    "eslint-config-check": "eslint --print-config . | eslint-config-prettier-check",
    "format:all": "run-s format:prettier format:eslint",
    "format:eslint": "eslint --fix \"**/*.js\"",
    "format:prettier": "prettier --write \"**/*.{js,scss,json,yaml,yml}\"",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\" --allow-empty-input",
    "lint:css:fix": "concurrently \"pnpm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:deps": "ember dependency-lint",
    "start": "concurrently 'ember serve' 'pnpm _syncPnpm --watch' --names 'serve,sync'",
    "test": "concurrently \"pnpm:lint\" \"pnpm:test:*(!ember-compatibility)\" --names \"lint,test:\"",
    "test:ember": "pnpm _syncPnpm && ember test",
    "test:ember-compatibility": "ember try:each",
    "_syncPnpm": "DEBUG=sync-pnpm pnpm sync-dependencies-meta-injected"
  },
  "lint-staged": {
    "*.{js,gjs}": "eslint --fix",
    "*.{js,gjs,scss,json,yaml,yml,md}": "prettier --write"
  },
  "dependencies": {
    "@babel/core": "^7.24.6"
  },
  "dependenciesMeta": {
    "ember-yeti-table": {
      "injected": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.24.6",
    "@babel/plugin-proposal-decorators": "^7.24.6",
    "@ember/optional-features": "^2.1.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "~3.3.0",
    "@embroider/test-setup": "^4.0.0",
    "@faker-js/faker": "^8.4.1",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "@glint/template": "~1.4.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-funnel": "^3.0.8",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-source": "^3.0.1",
    "concurrently": "^8.2.2",
    "ember-auto-import": "^2.7.3",
    "ember-cli": "~5.8.1",
    "ember-cli-addon-docs": "^7.0.1",
    "ember-cli-addon-docs-yuidoc": "^1.1.0",
    "ember-cli-babel": "~8.2.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-dependency-lint": "^2.0.1",
    "ember-cli-deploy": "^2.0.0",
    "ember-cli-deploy-build": "^3.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-mirage": "3.0.3",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-terser": "^4.0.2",
    "ember-composable-helpers": "^5.0.0",
    "ember-concurrency": "^4.0.2",
    "ember-data": "~4.12.5",
    "ember-lifeline": "^7.0.0",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^8.2.3",
    "ember-qunit": "^8.0.2",
    "ember-resolver": "^11.0.1",
    "ember-source": "~5.8.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-imports": "^4.1.1",
    "ember-template-lint": "^6.0.0",
    "ember-truth-helpers": "^4.0.3",
    "ember-try": "^3.0.0",
    "ember-yeti-table": "2.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^12.0.2",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-qunit": "~8.1.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "loader.js": "^4.7.0",
    "miragejs": "^0.1.48",
    "pnpm-sync-dependencies-meta-injected": "~0.0.14",
    "prettier": "^3.2.5",
    "prettier-plugin-ember-template-tag": "^2.0.2",
    "qunit": "^2.21.0",
    "qunit-dom": "^3.1.2",
    "rsvp": "^4.8.5",
    "sinon": "^18.0.0",
    "stylelint": "^16.6.1",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-prettier": "^5.0.0",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0"
  },
  "peerDependencies": {
    "ember-source": ">= 4.0.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "private": true
}
